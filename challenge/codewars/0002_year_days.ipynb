   "source": [
    "def years_days(year):\n",
    "    days = 365\n",
    "    if year % 4 == 0:\n",
    "        days += 1\n",
    "    if year % 100 == 0:\n",
    "        days -= 1\n",
    "    if year % 400 == 0:\n",
    "        days += 1\n",
    "    return (f'{year} has {days} days')\n",
    "\n",
    "years_days(0)"
   ]
  }
